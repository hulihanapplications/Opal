<h1>Login</h1>
<%= UserSession.find.record.inspect  %>
<br>
<%= @logged_in_user.inspect %>
<% form_for @user_session, :url => user_session_path do |f| %>
  <%= f.error_messages %>
  <%= f.label :username %><br />
  <%= f.text_field :username %><br />
  <br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  <br />
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
  <br />
  <%= f.submit "Login" %>
<% end %>